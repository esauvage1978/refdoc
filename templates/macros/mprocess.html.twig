{% macro views(items) %}
    {% import _self as macro %}
    <div class="list-group list-group-flush dropright">
        {% for item in items %}
            {{ macro.view(item) }}
        {% endfor %}
    </div>
{% endmacro %}

{% macro view(item) %}
    <a href="{{ path('admin_mprocess_show', {'id': item.id}) }}" class="list-group-item  card-animate ">
        <span class="text-bold {% if item.isEnable is same as(false) %}text-warning {% endif %}">
            {{ item.fullname | raw}}
        </span>
    </a>
{% endmacro %}